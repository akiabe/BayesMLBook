   "source": [
    "# Train data\n",
    "using Distributions\n",
    "N = 50; λ = 0.01\n",
    "Xᵗ = rand(Normal(25,√(1/λ)), N)\n",
    "\n",
    "# Hyperparameter\n",
    "m = 0; λₘ = 0.001\n",
    "\n",
    "# Training\n",
    "λ̂ₘ, m̂ = (N*λ+λₘ, (λ*sum(Xᵗ)+λₘ*m)/λ̂ₘ)\n",
    "\n",
    "# Prediction\n",
    "μₚ, λₚ = (m̂, λ*λ̂ₘ/(λ+λ̂ₘ))\n",
    "\n",
    "# Plot\n",
    "using PyPlot\n",
    "hist(rand(Normal(μₚ, √(1/λₚ)), 50));"
   ]
  }
 ],
